<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Manchito.Views.ViewPhoto"
             xmlns:local="clr-namespace:Manchito.ViewModel"
             Title="ViewPhoto">
    <ContentPage.BindingContext>
        <local:ViewPhotoViewModel></local:ViewPhotoViewModel>
    </ContentPage.BindingContext>
    <VerticalStackLayout>
        <Label 
            Text="Welcome to .NET MAUI!"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
        <Image WidthRequest="300" HeightRequest="600" x:Name="Photo1" Source="{Binding UrlPhoto}">

        </Image>
    </VerticalStackLayout>
    
</ContentPage>