<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
            xmlns:models="clr-namespace:Manchito.Model"
            xmlns:Views="clr-namespace:Manchito.Views"
            xmlns:local="clr-namespace:Manchito"
            x:Class="Manchito.MainPage"
            Title="Manchito"
             >
    <FlyoutPage.Flyout>
        <Views:FlyoutMenuPage x:Name="FlyoutPage" />
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <Views:ViewProjectList />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>